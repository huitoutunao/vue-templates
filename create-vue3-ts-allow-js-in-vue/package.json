    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.39.0",
    "eslint-plugin-vue": "^9.11.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "sass": "^1.62.0",
