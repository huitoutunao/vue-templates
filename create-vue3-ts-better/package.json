    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.45.0",
    "eslint-plugin-vue": "^9.15.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "typescript": "~4.7.4",
