{
  "name": "create-vue3-ts-better",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "cz": "czg",
    "dev": "vite --host",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --cache --cache-location ./node_modules/.cache/.eslintcache/ --fix --ignore-path .gitignore",
    "build": "vue-tsc --noEmit && vite build",
    "type-check": "vue-tsc --noEmit",
    "preview": "vite preview",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "prepare": "husky install"
  },
  "dependencies": {
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.2",
    "@types/node": "^18.16.19",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.1.3",
    "conventional-changelog-cli": "^3.0.0",
    "cz-git": "^1.7.0",
    "czg": "^1.7.0",
    "eslint": "^8.45.0",
    "eslint-plugin-vue": "^9.15.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "prettier": "^2.8.8",
    "sass": "^1.64.1",
    "typescript": "~4.7.4",
    "vite": "^4.4.5",
    "vite-plugin-checker": "^0.6.1",
    "vue-tsc": "^1.8.5"
  },
  "lint-staged": {
    "*.{vue,js,jsx,ts,tsx}": "npm run lint",
    "*.{vue,css,scss,less,styl,json}": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  },
  "engines": {
    "node": ">=16.14.0"
  }
}
